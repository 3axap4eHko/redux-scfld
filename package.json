{
  "name": "redux-scfld",
  "version": "0.5.3",
  "bin": {
    "redux": "./redux.js"
  },
  "description": "Redux Scaffold",
  "scripts": {
    "exec": "cross-env NODE_ENV=development babel-node src/redux.js",
    "build": "cross-env NODE_ENV=production gulp",
    "test": "cross-env NODE_ENV=development gulp && redux create unitTest:successCase && redux create unitTest:failureCase && redux create another:action && jasmine && eslint --fix spec/app/**",

{
  "name": "redux-scfld",
  "version": "2.0.6",
  "bin": {
    "redux": "./dist/redux.js"
  },
  "description": "Redux Scaffold",
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist --ignore __tests__,__fixtures__",
    "test": "rm -rf tmp && cross-env NODE_ENV=test jest --testPathPattern=unit",
    "intest": "rm -rf tmp && cross-env NODE_ENV=test jest src/__tests__/api.js",
    "patch-release": "npm run test && npm version patch && git push && git push --tags && npm run build && cd build && npm publish"
  },
  "keywords": [
    "redux",
    "scaffolder"
  ],
  "author": "Ivan Zakharchenko <3axap4eHko.development@gmail.com> (http://potofcode.com/)",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ],
  "peerDependencies": {
    "redux": "*",
    "redux-thunk": "*"
  },
  "dependencies": {
    "@std/esm": "^0.20.0",
    "commander": "^2.13.0",
    "del": "^3.0.0",
    "dot": "^1.1.2",
    "glob": "^7.0.5",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.0.1",
    "jest": "^22.1.4",
    "module-runtime": "^0.0.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3axap4eHko/redux-scfld.git"
  },
  "bugs": {
    "url": "https://github.com/3axap4eHko/redux-scfld/issues"
  },
  "homepage": "https://github.com/3axap4eHko/redux-scfld#readme"
}
